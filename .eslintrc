parser: "babel-eslint"

plugins: ["react"]

ecmaFeatures:
  jsx: true
  generators: true

env:
  browser: true
  jquery: true
  node: true

rules:
  quotes: [2, "single"]
  strict: [0, "global"]
  no-cond-assign: 0
  no-console: [1, {allow: ["warn", "error"]}]
  no-underscore-dangle: 0
  consistent-return: 0
  react/jsx-uses-react: 1
